@model IEnumerable<Models.Source>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FileName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Package)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FileName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Package)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PName)
        </td>
        <td>
            @Html.ActionLink("Edit Sorce", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Source Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete Source", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
